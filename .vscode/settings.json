{
  "explorer.openEditors.visible": 5,
  "files.exclude": {
    "**/.git": true,
    "**/*.js.map": true,
    "**/*.js": { "when": "$(basename).ts" },
    "e2e/**/*.js.map": true,
    "e2e/**/*.js": { "when": "$(basename).ts" }
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
